#include<stdio.h>
#include<stdlib.h>
void main()
{
	int i,x,pi,a[11];
	printf("input 10 numbers:\n");
	for(i=0;i<10;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("输入待插入的数，插入位置：");
	scanf("%d,%d",&x,&pi);
	if(pi<1||pi>11)
	{
		printf("插入位置有误\n");
		exit(0);
	}
	for(i=9;i>=pi-1;i--)
		a[i+1]=a[i];
	a[pi-1]=x;
	for(i=0;i<=10;i++)
		printf("%d",a[i]);
	printf("\n");
}
